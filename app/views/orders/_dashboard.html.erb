    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:status) %></th>
      <th><%= model_class.human_attribute_name(:total_price) %></th>
      <th><%= model_class.human_attribute_name(:created_at) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
      <tr>
    <% orders.each do |order| %>
        <td><%= link_to order.id, order_path(order) %></td>
        <td><%= order.status ? "Fullfilled" : "Unfullfilled" %></td>
        <td><%= order.total_price %></td>
        <td><%= order.created_at.strftime("%m/%d/%Y") %></td>
        <td>
          <%=link_to "fullfill order", edit_order_path(order), :class => 'btn btn-primary' unless order.status %>
          <%= button_to t('.destroy', :default => t("helpers.links.destroy")),order_path(order), :method => 'delete', :class => 'btn btn-danger' %>
        </td>
      </tr>
    <% end %>